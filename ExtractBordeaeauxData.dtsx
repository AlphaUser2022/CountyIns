            <component
              refId="Package\Data Flow Task 1\Source - Query"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Source - Query"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT CASE 
    WHEN brC.Trantype#  = 'Cancellation' THEN 'C' 
    WHEN brC.Trantype#  = 'Endorsement' THEN 'A' 
    WHEN brC.Trantype#  = 'Adjustment' THEN 'A' 
    WHEN brC.Trantype# = 'Renewal' THEN 'R' 
    ELSE 'N'
    END AS 'Trans_Type', 
    CONVERT(VARCHAR(10), brC.Dt_raised#, 103) AS 'Effective_date', 
    '7378' AS 'Scheme', 
    BDM.Cover# AS 'Cover', 
    ROW_NUMBER() OVER (ORDER BY brP.Polref@,brC.Trantype#) as 'Sequence', 
    CONVERT(VARCHAR(10), brP.Rdat#, 103) AS 'End_date',
    brP.Polno# AS 'Broker_Pol_ID', 
    REPLACE(BDM.Title#,',',' ') AS 'Title', 
    REPLACE(BDM.Surname#,',',' ') AS 'PH_surname', 
    REPLACE(BDM.First_name#,',',' ') AS 'PH_first_name', 
    REPLACE(BDM.Address1#,',',' ') AS 'Address_line1', 
    REPLACE(BDM.Address2#,',',' ') AS 'Address_line2', 
    BDM.Postcode# AS 'Postcode', 
    BDM.Regno# AS 'Registration', 
    BDM.Abi_code# AS 'ABI Code', 
    CASE
	WHEN BDM.Vehtype# = 'Lcv' THEN UPPER(BDM.Vehtype#)
	ELSE  BDM.Vehtype#
    END AS 'Vehicle_Type', 
    BDM.Vehmake# AS 'Make', 
    BDM.Vehmodel# AS 'Model', 
    BDM.Yearmake# AS 'Year_of_make', 
    BDM.Mileage# AS 'Annual_mileage', 
    BDM.Engcc# AS 'Cubic_capacity', 
    BDM.Gvw# AS 'Gross_weight', 
    BDM.Trans# AS 'Transmission', 
    BDM.Fuel# AS 'Fuel', 
    BDM.Mobile_number# AS 'Mobile Telephone Numbers', 
    brC.Orig_debt# AS 'Gross_premium', 
    brC.Comm_amt# AS 'Commission_premium',
    CASE
    WHEN brC.B@ = 00 THEN 18478
    WHEN brC.B@ = 01 THEN 18478
    WHEN brC.B@ = 03 THEN 18496
    WHEN brC.B@ = 06 THEN 18499
    WHEN brC.B@ = 07 THEN 18503
    WHEN brC.B@ = 08 THEN 18495
    WHEN brC.B@ = 09 THEN 18492
    WHEN brC.B@ = 10 THEN 18479
    WHEN brC.B@ = 11 THEN 18493
    WHEN brC.B@ = 12 THEN 18497
    WHEN brC.B@ = 13 THEN 18500
    WHEN brC.B@ = 15 THEN 18501
    WHEN brC.B@ = 16 THEN 18504
    WHEN brC.B@ = 17 THEN 18502
    WHEN brC.B@ = 18 THEN 18508
    WHEN brC.B@ = 19 THEN 18505
    WHEN brC.B@ = 20 THEN 18498
    END AS AGENCYNO,
    BDM.Poltype# AS 'PH_Type', 
    BDM.Company_name# AS 'companyName'
    FROM
    brcledger brC 
    INNER JOIN brpolicy brP ON brC.B@ = brP.B@
    AND brC.PolRef@ = brP.PolRef@
    INNER JOIN BD_MBEQ BDM  ON brP.B@ = BDM.B@
    AND brP.PolRef@ = BDM.PolRef@
    WHERE
    brC.B@ NOT IN(98)
    AND (brC.Trantype#&lt;&gt;'Charge' AND brC.Trantype# &lt;&gt; 'Renewal' OR (brC.Trantype# = 'Renewal' AND brC.Ledger_dt# is not null))
    AND brC.Dt_raised# = Convert (Date,GetDate())</property>
